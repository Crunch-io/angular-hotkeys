{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "extends": "eslint:recommended",
    "globals": {
        "angular": true,
        "Mousetrap": true
    },
    "rules": {
         "indent": ["error", 4, {
            "SwitchCase": 1
        }],
        "no-var": "error",
        "prefer-const": "error"
    }
}